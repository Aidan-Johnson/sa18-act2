<h1>ALL OUR PRODUCTS</h1>
<div class="table-responsive">
  <table class="table table-striped table-hover">

    <thead class="table-dark">
      <tr>
        <th>Name</th>
        
      </tr>
    </thead>

    <tbody>
      <% @products.each do |p| %>
        <tr>
          <td><%= link_to p.name, product_path(p)%></td>
        </tr>
      <% end %>
    </tbody>

  </table>
</div>